    "url": "sane.jp"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "electron-debug": "^0.1.1",
    "formsy-react": "^0.14.1",
    "lovefield": "2.0.53",
    "react": "^0.13.3",
    "react-redux": "^0.2.2",
    "react-router": "^1.0.0-beta3",
    "redux": "^1.0.0-rc",
    "redux-devtools": "^0.1.2",
    "redux-react-router": "^0.2.1",
    "redux-thunk": "^0.1.0"
  },
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-core": "^5.8.19",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.19",
    "conventional-changelog": "^0.2.1",
    "cpy": "^3.4.0",
    "eclint": "^0.2.6",
    "electron-packager": "^5.0.1",
    "electron-prebuilt": "^0.30.1",
    "eslint": "^1.0.0-rc",
    "eslint-loader": "^0.14.2",
    "eslint-plugin-babel": "babel/eslint-plugin-babel",
    "eslint-plugin-react": "^3.1.0",
    "espower-loader": "^0.11.0",
    "fixpack": "^2.2.0",
    "globstar": "^1.0.0",
    "html-webpack-plugin": "^1.6.0",
    "json-loader": "^0.5.2",
    "mocha": "^2.2.5",
    "npm-check-updates": "^2.0.1",
    "power-assert": "^0.11.0",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.4.2",
    "touch": "^1.0.0",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "keywords": [
    "electron-app"
  ],
  "license": "MIT",
  "main": "main.js",
  "private": true,
  "productName": "TriageForGitHub",
  "repository": "lyrictenor/electron-triage-for-github",
  "scripts": {
    "build": "npm run build:dist && rimraf output && cd dist && electron-packager ./ $npm_package_productName --out=../output --platform=darwin --arch=x64 --version=0.30.1 && cd ..",
    "build:dev": "npm run clean && npm run resource && webpack --config webpack/webpack.application.config.dev.js",
    "build:dist": "npm run clean && npm run resource && webpack --config webpack/webpack.application.config.production.js",
    "build:local": "npm run clean && npm run resource && webpack --config webpack/webpack.application.config.js",
    "changelog": "touch changelog.md && conventional-changelog -i changelog.md --overwrite --preset angular",
    "clean": "rimraf dist",
    "eclint": "globstar --ignore \"node_modules/**\" -- eclint check \"*.{js,yml,md,json}\"",
    "electron": "npm run build:dev && cd dist && electron . && cd ..",
    "eslint": "eslint --ext .js,.jsx lib",
    "fixpack": "fixpack",
    "git:tag": "git tag v${npm_package_version}",
    "lint": "npm run eclint && npm run eslint",
    "ncu": "npm-check-updates -ua",
    "resource": "cpy package.json dist && npm run resource:main",
    "resource:main": "cpy lib/main.js dist && cd dist && npm install menubar electron-debug && cd ..",
    "start": "npm run build:local && node lib/server.js",
    "test": "mocha --require ./enable-power-assert.js -- ./test/*test.js",
    "verify": "npm run lint && npm run build:local && npm run build:dev && npm run build:dist"
  }
}
